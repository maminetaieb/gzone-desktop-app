<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="300.0" prefWidth="400.0" style="-fx-background-color: #3c3c3c;" styleClass="mainFxmlClass" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.gzone.ChangePasswordController">
    <stylesheets>
        <URL value="@/styles/changepassword.css" />
    </stylesheets>
   <children>
      <PasswordField fx:id="newpassword" layoutX="102.0" layoutY="106.0" prefHeight="25.0" prefWidth="197.0" promptText="new password.." />
      <PasswordField fx:id="confirmpassword" layoutX="102.0" layoutY="164.0" prefHeight="25.0" prefWidth="197.0" promptText="confirm password.." />
      <Label layoutX="126.0" layoutY="51.0" text="Enter new password" textFill="WHITE">
         <font>
            <Font name="Impact" size="18.0" />
         </font>
      </Label>
      <Button fx:id="submit" layoutX="174.0" layoutY="224.0" mnemonicParsing="false" onAction="#changepassword" style="-fx-background-color: #77b700;" text="Submit" textFill="WHITE">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
