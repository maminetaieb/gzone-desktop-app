<AnchorPane id="AnchorPane" fx:id="APCreate" prefHeight="600.0" prefWidth="800.0" stylesheets="@../../../styles/HomePage.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.gzone.CreateGameController">
