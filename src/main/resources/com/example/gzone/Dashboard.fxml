            <TableColumn fx:id="tbirthdate" prefWidth="71.199951171875" text="birthdate" />
            <TableColumn fx:id="tjoindate" prefWidth="67.20001220703125" text="join date" />
            <TableColumn fx:id="tphoto" minWidth="0.0" prefWidth="58.4000244140625" text="photo" />
            <TableColumn fx:id="trole" minWidth="0.0" prefWidth="73.5999755859375" text="role" />
