<AnchorPane id="AnchorPane" prefHeight="600.0" prefWidth="800.0" styleClass="mainFxmlClass" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.gzone.GameViewController">
    <stylesheets>
        <URL value="@homepage.css" />
    </stylesheets>
   <children>
      <VBox prefHeight="604.0" prefWidth="164.0" style="-fx-background-color: #3c3c3c;" AnchorPane.leftAnchor="0.0">
         <children>
            <Circle fill="#00ff02" radius="60.0" stroke="BLACK" strokeType="INSIDE">
               <VBox.margin>
                  <Insets left="15.0" top="40.0" />
               </VBox.margin>
            </Circle>
            <ImageView fitHeight="110.0" fitWidth="111.0" pickOnBounds="true" preserveRatio="true">
               <VBox.margin>
                  <Insets left="20.0" top="-115.0" />
               </VBox.margin>
               <image>
                  <Image url="@../../../img/logo-gzone.png" />
               </image>
            </ImageView>
            <Button mnemonicParsing="false" onAction="#HomePage" onMouseClicked="#HomePage" prefHeight="17.0" prefWidth="96.0" style="-fx-background-color: transparent;" text="HomePage" textFill="#fffefe">
               <padding>
                  <Insets bottom="20.0" top="20.0" />
               </padding>
               <VBox.margin>
                  <Insets left="30.0" top="60.0" />
               </VBox.margin>
               <font>
                  <Font name="System Bold" size="14.0" />
               </font>
            </Button>
            <Button layoutX="40.0" layoutY="160.0" mnemonicParsing="false" onAction="#Team" onMouseClicked="#Team" prefHeight="17.0" prefWidth="96.0" style="-fx-background-color: transparent;" text="Team" textFill="#fffefe">
               <padding>
                  <Insets bottom="20.0" top="20.0" />
               </padding>
               <font>
                  <Font name="System Bold" size="14.0" />
               </font>
               <VBox.margin>
                  <Insets left="20.0" />
               </VBox.margin>
            </Button>
            <Button layoutX="10.0" layoutY="227.0" mnemonicParsing="false" onAction="#Tournament" onMouseClicked="#Tournament" prefHeight="17.0" prefWidth="96.0" style="-fx-background-color: transparent;" text="Tournament" textFill="#fffefe">
               <padding>
                  <Insets bottom="20.0" top="20.0" />
               </padding>
               <font>
                  <Font name="System Bold" size="14.0" />
               </font>
               <VBox.margin>
                  <Insets left="30.0" />
               </VBox.margin>
            </Button>
            <Button layoutX="10.0" layoutY="294.0" mnemonicParsing="false" onAction="#Store" onMouseClicked="#Store" prefHeight="17.0" prefWidth="96.0" style="-fx-background-color: transparent;" text="Store" textFill="#fffefe">
               <padding>
                  <Insets bottom="20.0" top="20.0" />
               </padding>
               <font>
                  <Font name="System Bold" size="14.0" />
               </font>
               <VBox.margin>
                  <Insets left="30.0" />
               </VBox.margin>
            </Button>
            <Button layoutX="10.0" layoutY="361.0" mnemonicParsing="false" onAction="#form" onMouseClicked="#Forum" prefHeight="17.0" prefWidth="96.0" style="-fx-background-color: transparent;" text="Forum" textFill="#fffefe">
               <padding>
                  <Insets bottom="20.0" top="20.0" />
               </padding>
               <font>
                  <Font name="System Bold" size="14.0" />
               </font>
               <VBox.margin>
                  <Insets left="30.0" />
               </VBox.margin>
            </Button>
         </children>
      </VBox>
      <AnchorPane fx:id="gamepane" layoutX="164.0" prefHeight="604.0" prefWidth="742.0">
         <children>
            <HBox alignment="CENTER" layoutX="15.0" layoutY="147.0" minHeight="-Infinity" minWidth="-Infinity" prefHeight="41.0" prefWidth="357.0" stylesheets="@fxml.css">
               <padding>
                  <Insets left="30.0" right="30.0" />
               </padding>
               <children>
                  <TextField fx:id="tfSearch" prefHeight="40.0" prefWidth="245.0" promptText="Type here to search" styleClass="tranparant" stylesheets="@fxml.css">
                     <font>
                        <Font name="Cambria" size="18.0" />
                     </font>
                  </TextField>
                  <Button fx:id="btnSearch" mnemonicParsing="false" onAction="#find" prefHeight="31.0" prefWidth="115.0" styleClass="search-button" stylesheets="@fxml.css" text="SEARCH">
                     <HBox.margin>
                        <Insets />
                     </HBox.margin>
                     <font>
                        <Font name="Roboto Black" size="14.0" />
                     </font>
                  </Button>
               </children>
               <opaqueInsets>
                  <Insets />
               </opaqueInsets>
               <styleClass>
                  <String fx:value="search-box" />
                  <String fx:value="shadow" />
               </styleClass>
            </HBox>
            <Button fx:id="btnAddGame" layoutX="577.0" layoutY="32.0" mnemonicParsing="false" onAction="#AddGame" styleClass="add-button" stylesheets="@fxml.css" text="Add New Game" textFill="#336861">
               <font>
                  <Font name="Roboto Black" size="15.0" />
               </font></Button>
            <Button fx:id="BTNEdit" layoutX="417.0" layoutY="538.0" mnemonicParsing="false" onAction="#edit" prefHeight="31.0" prefWidth="82.0" styleClass="edit-button" stylesheets="@fxml.css" text="EDIT" textFill="#336861">
               <font>
                  <Font name="Roboto Black" size="15.0" />
               </font></Button>
            <Button fx:id="btnDelete" layoutX="209.0" layoutY="535.0" mnemonicParsing="false" onAction="#delete" prefHeight="31.0" prefWidth="82.0" stylesheets="@fxml.css" text="DELETE" textFill="#336861">
               <font>
                  <Font name="Roboto Black" size="15.0" />
               </font>
               <styleClass>
                  <String fx:value="delete-button" />
                  <String fx:value="delete-button" />
               </styleClass></Button>
            <ListView fx:id="GameList" layoutX="12.0" layoutY="215.0" onMouseClicked="#UpdateNumberOfTournament" prefHeight="291.0" prefWidth="529.0" />
            <ListView fx:id="Top5" layoutX="554.0" layoutY="245.0" prefHeight="200.0" prefWidth="164.0" />
            <Label fx:id="MyLabel" layoutX="246.0" layoutY="41.0" prefHeight="29.0" prefWidth="164.0" text="Our Games ">
               <font>
                  <Font name="Arial Black" size="24.0" />
               </font>
            </Label>
         </children>
      </AnchorPane>
      <Text fx:id="tnot" fill="#336861" layoutX="546.0" layoutY="129.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Number Of Tournaments">
         <font>
            <Font name="Arial Bold" size="15.0" />
         </font></Text>
      <Text fx:id="Tourid" layoutX="750.0" layoutY="129.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Text">
         <font>
            <Font name="Arial" size="15.0" />
         </font></Text>
   </children>
</AnchorPane>
