         <font>
            <Font size="18.0" />
         </font>
      </Text>
      <ImageView fitHeight="87.0" fitWidth="104.0" layoutX="194.0" layoutY="13.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../../img/store-logo.png" />
         </image>
      </ImageView>
      <ListView fx:id="tbview" layoutX="217.0" layoutY="269.0" prefHeight="250.0" prefWidth="514.0" />
      <Button fx:id="btnlike" layoutX="251.0" layoutY="537.0" mnemonicParsing="false" onAction="#handleLike" prefHeight="25.0" prefWidth="61.0" text="Like" />
      <Button fx:id="btndislike" layoutX="370.0" layoutY="537.0" mnemonicParsing="false" onAction="#handleDislike" prefHeight="25.0" prefWidth="61.0" text="Dislike" />
