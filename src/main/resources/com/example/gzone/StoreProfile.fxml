         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Button>
      <Button fx:id="deletestorebutton" layoutX="204.0" layoutY="161.0" mnemonicParsing="false" onAction="#DeleteYourStore" prefHeight="31.0" prefWidth="120.0" text="Delete Store " />
