                        <Insets bottom="20.0" top="20.0" />
                    </padding>
                    <font>
                        <Font name="System Bold" size="14.0" />
                    </font>
                    <VBox.margin>
                        <Insets left="30.0" />
                    </VBox.margin>
                </Button>
                <Button layoutX="10.0" layoutY="361.0" mnemonicParsing="false" onAction="#Forum" prefHeight="17.0" prefWidth="96.0" style="-fx-background-color: transparent;" text="Forum" textFill="#fffefe">
                    <padding>
                        <Insets bottom="20.0" top="20.0" />
                    </padding>
                    <font>
                        <Font name="System Bold" size="14.0" />
                    </font>
                    <VBox.margin>
                        <Insets left="30.0" />
                    </VBox.margin>
                </Button>
            </children>
        </VBox>
      <Button fx:id="addbutton" layoutX="447.0" layoutY="158.0" mnemonicParsing="false" onAction="#AddToStore" prefHeight="37.0" prefWidth="110.0" style="-fx-background-color: #86d300;" text="Add Your Item" textFill="#fffefe">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Button>
      <Button fx:id="deletebutton" layoutX="557.0" layoutY="556.0" mnemonicParsing="false" onAction="#DeleteYourItem" prefHeight="37.0" prefWidth="120.0" text="Delete Your Item" />
