                  <AnchorPane prefHeight="461.0" prefWidth="665.0" style="-fx-background-color: 1f7ea1;">
                     <children>
                        <VBox layoutX="50.0" layoutY="419.0" prefHeight="22.0" prefWidth="165.0" />
